    position: fixed;
    top: 38.5px;
    left: 50%;
    margin: 0 0 0 -130px !important;
    padding: 10px 0 0 0;
    width: 260px;
    min-height: 32px;
    text-align: center;
    z-index: 1001;
    list-style: none;
    display: none;
    font-size: 14px;
}
  #mce-success-response {
        background: rgba(0,0,0,0.67);   
        color: #fff;
  }
