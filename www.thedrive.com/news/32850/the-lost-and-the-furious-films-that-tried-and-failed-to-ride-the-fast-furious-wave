Obj = feAdSlotsConfig[adSlotKey];if (!slotObj) {console.log("skipping define for " + divId + " / " + adSlotKey);return false;}googletag.cmd.push(function() {console.log("CALL: googletag.defineSlot for " + adSlotKey + " on " + divId, slotObj.sizes);var slot = googletag.defineSlot(adSlotKey, slotObj.sizes, divId);if ( slot ) {slot.addService(googletag.pubads());feAdSlotsConfig[adSlotKey].slots[divId] = slot;feAdSlotsConfig[adSlotKey].viewability[divId] = {viewableSince: null,timer: null,totalViewableTime: 0};if ( ENABLE_AMAZON ) {console.log("Defining A9 for " + adSlotKey + " on div " + divId);apstagSlots.push({slotID: divId,sizes: feAdSlotsConfig[adSlotKey].sizes,slotName: adSlotKey});}}});return true;};function buildBids(slotObj, slotName) {var bids = [];if ( ENABLE_PREBID ) {if (slotObj.pbjsBid_districtM_placementId) {bids.push({bidder: 'districtm',params: {placementId: slotObj.pbjsBid_districtM_placementId}});}if (slotObj.pbjsBid_districtMDMX_params) {bids.push({bidder: 'districtmDMX',params: slotObj.pbjsBid_districtMDMX_params});}if (slotObj.pbjsBid_appnexus_params) {bids.push({bidder: 'appnexus',params: slotObj.pbjsBid_appnexus_params});}if (slotObj.pbjsBid_rubicon_params) {bids.push({bidder: 'rubicon',params: slotObj.pbjsBid_rubicon_params});}if (slotObj.pbjsBid_ix_params) {slotObj.sizes.forEach(function (size) {bids.push({bidder: 'ix',params: {siteId: slotObj.pbjsBid_ix_params.siteId,size: size}});});}if (slotObj.pbjsBid_sovrn_params) {slotObj.pbjsBid_sovrn_params.forEach(function (tagId) {bids.push({bidder: 'sovrn',params: {tagid: tagId}});});}if (slotObj.pbjsBid_openx_params) {bids.push({bidder: 'openx',params: {delDomain: 'thedrive-d.openx.net', unit: slotObj.pbjsBid_openx_params}});}if ( ENABLE_RHYTHM_ONE ) {bids.push({bidder: 'rhythmone', params: {placementId: '83228'}});}if ( ENABLE_EMX ) {if (slotObj.pbjsBid_emx_params) {bids.push({bidder: 'emx_digital',params: {tagid: slotObj.pbjsBid_emx_params}});}}if ( ENABLE_CONVERSANT ) {bids.push({bidder: 'convers