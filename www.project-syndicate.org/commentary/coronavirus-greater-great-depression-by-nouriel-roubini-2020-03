    <figure class="image image--ratio-16-9  " >

        <picture >

                            <source data-srcset="https://webapi.project-syndicate.org/library/fcfd4629bc3ce3932ce5e3df9ff24e2a.16-9-medium.1.jpg" media="--xsmall" />
