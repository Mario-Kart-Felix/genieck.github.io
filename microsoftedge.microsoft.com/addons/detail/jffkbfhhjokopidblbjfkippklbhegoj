    <script type="text/javascript" nonce=KIX9JUylM8iFJBeSYv5VC7&#x2B;rCf9sY2NDiMHft3NoCeo= >
        window.performance.mark('ExtensionStore_IndexHtmlLoadEnd');
        window.performance.mark('ExtensionStore_JSLoadStart');
        document.addEventListener('DOMContentLoaded', renderBetaBranding);
        var chrome = window.chrome;
        function renderBetaBranding() {

            if (document.getElementById("uhfCatLogo")) {

                // Create superscript element for desktop view
                var betaDesktopElement = document.createElement("SPAN");
                betaDesktopElement.style.fontSize = "9px";
                betaDesktopElement.style.marginTop = "16px";
                betaDesktopElement.style.fontWeight = "500";
                betaDesktopElement.classList.add("x-hidden-vp-mobile-st");
                betaDesktopElement.innerHTML = "BETA";
                betaDesktopElement.setAttribute("aria-label", "");
                betaDesktopElement.setAttribute("aria-hidden", "true");

                // Appending elements and setting aria-labels for screen readers for desktop view 
                document.getElementById("uhfCatLogo").style.setProperty("padding-right", "3px", "important")

                if (document.getElementById("uhfCatLogo").parentNode) {
                    document.getElementById("uhfCatLogo").parentNode.insertAdjacentElement('afterend', betaDesktopElement);
                }
                document.getElementById("uhfCatLogo").setAttribute("aria-label", "microsoft edge addons beta");
            }

            if (document.getElementById("uhfCatLogoButton")) {

                // Create superscript element for mobile view
                var betaMobileElement = document.createElement("SUP");
                betaMobileElement.innerHTML = "BETA";
                betaMobileElement.style.fontSize = "9px";
                betaMobileElement.setAttribute("aria-label", "");
       
                // Appending elements and setting aria-labels for screen readers for mobile view 
                document.getElementById("uhfCatLogoButton").appendChild(betaMobileElement)
                document.getElementById("uhfCatLogoButton").setAttribute("aria-label", "microsoft edge addons beta");
            }

            // Remove event listener
            document.removeEventListener('DOMContentLoaded', renderBetaBranding);
        }
    </script>
