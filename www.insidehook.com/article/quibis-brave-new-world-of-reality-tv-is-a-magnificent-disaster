				<div class="card-section">
				<div class="card-content">
					<a href="https://www.insidehook.com/article/vehicles/review-2020-toyota-tundra-truck-city?utm_source=direct&utm_medium=module&utm_campaign=recirc&utm_content=rightrailmostpopular">
																			<source media="(min-width: 768px)"
								srcset="https://www.insidehook.com/wp-content/uploads/2020/04/Tundra_Hero_Toyota_Crewmax_2020.jpg?resize=450%2C300 450w, https://www.insidehook.com/wp-content/uploads/2020/04/Tundra_Hero_Toyota_Crewmax_2020.jpg?resize=750%2C500 1200w"
								sizes="(min-width: 768rem) 450px, 100vw">
							<!-- Small art direction -->
							<source
								srcset="https://www.insidehook.com/wp-content/uploads/2020/04/Tundra_Hero_Toyota_Crewmax_2020.jpg?resize=150%2C150 150w"
