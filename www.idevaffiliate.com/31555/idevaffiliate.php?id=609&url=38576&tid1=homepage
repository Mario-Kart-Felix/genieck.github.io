  try {
    if (window.localStorage.getItem('fs_enabled') === null) {
      window.localStorage.setItem('fs_enabled', Math.ceil(Math.random() * 20) === 1)
    } else if (window.__PRELOADED_STATE__ &&
      window.__PRELOADED_STATE__.experiments &&
      window.__PRELOADED_STATE__.experiments.checkoutV2 &&
      window.__PRELOADED_STATE__.experiments.checkoutV2.alternative === 'refresh') {
      window.localStorage.setItem('fs_enabled', true)
    }

    return JSON.parse(window.localStorage.getItem('fs_enabled'));
  } catch(_) {
    return false;
