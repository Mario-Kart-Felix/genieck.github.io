  <source srcset="https://cdn.vox-cdn.com/thumbor/hGTcChBKamXUWGOL7UvWxJ2SlBQ=/0x0:3000x2000/320x213/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 320w, https://cdn.vox-cdn.com/thumbor/M-xbcqS9Y3njurS_4nzXJTUUOZg=/0x0:3000x2000/620x413/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 620w, https://cdn.vox-cdn.com/thumbor/DroCukDwhs4kwYnU3LvWUY4xprU=/0x0:3000x2000/920x613/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 920w, https://cdn.vox-cdn.com/thumbor/uup45m3_QgIvbwrgTFv70MAkeJk=/0x0:3000x2000/1220x813/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1220w, https://cdn.vox-cdn.com/thumbor/aLEcyC0-lnj7bJHeAEhq6Wwev5E=/0x0:3000x2000/1520x1013/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1520w, https://cdn.vox-cdn.com/thumbor/5ARHo6JQ27p1FrupTXJZEqmHRyI=/0x0:3000x2000/1820x1213/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1820w, https://cdn.vox-cdn.com/thumbor/W0btxM1cjOXU2L5rqVNtBLCIXu8=/0x0:3000x2000/2120x1413/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 2120w, https://cdn.vox-cdn.com/thumbor/Y63vBIGDBblkPULRNZG8hZwvdyo=/0x0:3000x2000/2420x1613/filters:focal(1256x697:1736x1177):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 2420w" sizes="(min-width: 1221px) 846px, (min-width: 880px) calc(100vw - 334px), 100vw" type="image/webp">


<img srcset="https://cdn.vox-cdn.com/thumbor/ba12Pb5-uD1efPBx7gLv4Icz6X0=/0x0:3000x2000/320x213/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 320w, https://cdn.vox-cdn.com/thumbor/Eop2TtY1Y0BAnDr671wPh9m1PVk=/0x0:3000x2000/620x413/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 620w, https://cdn.vox-cdn.com/thumbor/78mproa3xKEgVgy0el0u4bfUtDw=/0x0:3000x2000/920x613/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 920w, https://cdn.vox-cdn.com/thumbor/qqREmjULa7K6TKLNBEJnDm6ZMJk=/0x0:3000x2000/1220x813/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1220w, https://cdn.vox-cdn.com/thumbor/-BBZmWo5ihNeh6ZpCDTx9JN5cJk=/0x0:3000x2000/1520x1013/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1520w, https://cdn.vox-cdn.com/thumbor/gF3vw7Ryg3bmeZeC5gtZuQluEhE=/0x0:3000x2000/1820x1213/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 1820w, https://cdn.vox-cdn.com/thumbor/DTqR-mPe_bvaiA4qxLYqk1ePmrM=/0x0:3000x2000/2120x1413/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 2120w, https://cdn.vox-cdn.com/thumbor/zOf8l9GEaqYT5zwGkiUCwNrgtGU=/0x0:3000x2000/2420x1613/filters:focal(1256x697:1736x1177)/cdn.vox-cdn.com/uploads/chorus_image/image/66592151/mike_schur_interview_getty_nbc_ringer.0.jpg 2420w" sizes="(min-width: 1221px) 846px, (min-width: 880px) calc(100vw - 334px), 100vw" alt="" data-upload-width="3000" src="https://cdn.vox-cdn.com/thumbor/00o5UZWNbMMK_psUa0TJQ8WR1pQ=/0x0:3000x2000/1200x800/filters:focal(1256x