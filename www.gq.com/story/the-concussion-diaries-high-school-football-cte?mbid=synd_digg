so-summary-collection-row":{"sm":{"aspectRatio":"1:1","width":160},"xxl":{"aspectRatio":"1:1","width":320}},"verso-river":{"sm":{"aspectRatio":"1:1","width":160},"lg":{"aspectRatio":"16:9","width":768}},"summaryCollageOne":{"sm":{"aspectRatio":"1:1","width":768},"lg":{"aspectRatio":"16:9","width":1280},"xxl":{"aspectRatio":"16:9","width":1600}},"verso-ticker":{"sm":{"aspectRatio":"master","width":160},"md":{"aspectRatio":"master","width":160},"lg":{"aspectRatio":"master","width":160},"xl":{"aspectRatio":"master","width":160}},"summaryCollageThree":{"primary":{"sm":{"aspectRatio":"1:1","width":768},"lg":{"aspectRatio":"1:1","width":768},"xxl":{"aspectRatio":"1:1","width":1024}},"secondary":{"sm":{"aspectRatio":"16:9","width":320},"lg":{"aspectRatio":"16:9","width":640}}},"summaryCollectionGrid":{"sm":{"aspectRatio":"1:1","width":768},"lg":{"aspectRatio":"4:3","width":1280},"xxl":{"aspectRatio":"4:3","width":1600}},"summaryCollageFive":{"primary":{"sm":{"aspectRatio":"1:1","width":768},"lg":{"aspectRatio":"1:1","width":768},"xxl":{"aspectRatio":"1:1","width":1024}},"secondary":{"sm":{"aspectRatio":"16:9","width":320},"lg":{"aspectRatio":"16:9","width":640}}},"summaryList":{"sm":{"aspectRatio":"1:1","width":768},"lg":{"aspectRatio":"4:3","width":1280},"xxl":{"aspectRatio":"4:3","width":1600}},"summaryListWithAside":{"sm":{"aspectRatio":"16:9","width":768},"lg":{"aspectRatio":"16:9","width":768}},"tout":{"sm":{"aspectRatio":"16:9","width":1280},"xxl":{"aspectRatio":"16:9","width":1280}}}},"transformed":{"ads.page":{"channel":"culture","contentType":"article","keywords":{"copilotid":["585054d2524455347e621614"],"platform":["verso"],"tags":["culture","cte","sports","football","print","longform","concussions","documentary","video","best-of-gq"]},"server":"production","slug":"the-concussion-diaries-high-school-football-cte","subChannel":"","templateType":"mt_article"},"payment":{"form":"free","acceptableFormsOfTenderedPayment":["free"],"acceptableForms":[],"acceptableScopes