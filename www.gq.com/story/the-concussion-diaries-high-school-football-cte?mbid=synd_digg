aryLinks":[{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Style","url":"\u002Fstyle","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Grooming","url":"\u002Fgrooming","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Recommends","url":"\u002Fgq-recommends","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Culture","url":"\u002Fculture","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Fitness","url":"\u002Ffitness","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"GQ Sports","url":"\u002Fabout\u002Fgqsports","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Videos","url":"https:\u002F\u002Fvideo.gq.com\u002F","forceLeftOfNav":false},{"dangerousIcon":"","isExternal":false,"showInTopNav":true,"text":"Shop","url":"https:\u002F\u002Fwww.gq.com\u002Frecommends-shop\u002F","forceLeftOfNav":false}],"searchLink":"\u002Fsearch","secondaryLinks":[],"callToActionLink":null,"pageHeadline":"The Concussion Diaries: One High School Football Player’s Secret Struggle with CTE","socialLinks":[{"network":"instagram","url":"https:\u002F\u002Finstagram.com\u002Fgq\u002F","label":"Follow us on Instagram"},{"network":"youtube","url":"https:\u002F\u002Fwww.youtube.com\u002Fuser\u002Fgqvideos\u002F","label":"Follow us on YouTube"},{"network":"facebook","url":"https:\u002F\u002Fwww.facebook.com\u002Fgq\u002F","label":"Follow us on Facebook"},{"network":"twitter","url":"https:\u002F\u002Ftwitter.com\u002Fgqmagazine\u002F","label":"Follow us on Twitter"}],"utilityLinks":[{"text":"Subscribe","url":"https:\u002F\u002Fsubscribe.gq.com\u002Fsubscribe\u002Fgq\u002F120623?source=HCL_GQM_GLOBAL_NAVBAR_0_REDIRECT_ZZ","forceLeftOfNav":false,"showInTopNav":true,"shouldRenderCMUnit":true},{"text":"Newsletter","url":"\u002Fnewsletter\u002Fsubs