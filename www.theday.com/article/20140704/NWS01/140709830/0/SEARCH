	$td(document).ready(function() {
		if($td(".fullStory p").length) {
			var pCount = $td(".fullStory p");
			if (pCount.length<3) {
				var pLength;
				var bigP;
				pCount.each(function(n) {
					if(n) {
						if (pLength<$td(this).text().length) {
							pLength = $td(this).text().length;
							bigP = n;
						}
					}
					else {
						pLength = $td(this).text().length;
						bigP = 0;
					}
				});
				var splitBigP = $td(".fullStory p:eq("+bigP+")").html().split(/<br><br>/);
				if(splitBigP.length>2) {
					$td(".fullStory").html("");
					for (para in splitBigP) {
						$td(".fullStory").append("<p>"+splitBigP[para]+"</p>");
					}
				}
			}
			var twitterEncodedURL = encodeURI("https://www.theday.com/article/20140704/"+catId+"/140709830");
