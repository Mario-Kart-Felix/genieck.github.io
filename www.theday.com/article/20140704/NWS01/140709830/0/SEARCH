					//font size selector
					 if($td.cookie('TEXT_SIZE')) {
					  $td('body').addClass($td.cookie('TEXT_SIZE'));
					  $td('.'+$td.cookie('TEXT_SIZE')).parent().addClass('active');
					 }
					 else {
						$td('body').addClass('medium');
						$td('.medium').parent().addClass('active');
					 }
					 $td('.resizer button').click(function() {
					  $td('.resizer .active').removeClass('active');
					  var textSize = $td(this).find('span').attr('class');
					  $td('body').removeClass('small medium large').addClass(textSize);
					  $td('.'+textSize).parent().addClass('active');
					  $td.cookie('TEXT_SIZE',textSize, { path: '/', expires: 10000 });
					  return false;
					 });
					</script> 
