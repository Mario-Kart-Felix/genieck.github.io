<script>
$td(window).scroll(function(){
    if ($td(window).scrollTop() > 280) {
        $td('#slidelogo').fadeIn(800);
        $td('#topnavbar').css('top','0px');
    }
    else 
    {
         $td('#slidelogo').fadeOut(800);
         $td('#topnavbar').css('top','0px');
    }
});
