					//end new
					crossDomain: true,
					contentType: "application/json; charset=utf-8",
					dataType: "jsonp",
					data: {fields: 'engagement', access_token: token, id: surl2},
					success: function(data) {
							if(data.engagement.share_count) {
								$td("#fbshares").html(data.engagement.share_count).addClass("badge");
							}
							if (data.engagement.share_count>1000) {
								$td("#facebookShare").css("paddingRight","7px");
							}
							else if(data.engagement.share_count>100) {
								$td("#facebookShare").css("paddingRight","2px");
							}
							else {
								$td("#facebookShare").css("paddingRight","0px");
							}
					},
					error: function(xhr, status, error) { 
						console.log(xhr+','+status+','+error); 
					},
					async: true,
					cache: false
				});
			$td("#facebookShare a").attr("href","https://www.facebook.com/sharer/sharer.php?u="+twitterEncodedURL);
			var encodedHead = encodeURI("Suspicious Flyer Found in Guilford Mailboxes");
