    <script>
    $td(document).ready(function(){												
       //Navigation Menu Slider
        $td('#nav-expander').on('click',function(e){
      		e.preventDefault();
      		$td('body').toggleClass('nav-expanded');
      	});
      	$td('#nav-close').on('click',function(e){
      		e.preventDefault();
      		$td('body').removeClass('nav-expanded');
      	});
      	// Initialize navgoco with options
        $td(".main-menu").navgoco({
            caret: '<span class="caret"></span>',
            accordion: true,
            openClass: 'open',
            save: true,
            cookie: {
                name: 'navgoco',
                expires: false,
                path: '/'
            },
            slide: {
                duration: 300,
                easing: 'swing'
            }
        });
        $td(".ad-menu").navgoco({
            caret: '<span class="caret"></span>',
            accordion: true,
            openClass: 'open',
            save: true,
            cookie: {
                name: 'navgoco',
                expires: false,
                path: '/'
            },
            slide: {
                duration: 300,
                easing: 'swing'
            }
        });
      });
// Date field
	$td(document).ready(function() {
		//populate the date field
		$td(".dateline").html(moment().format('dddd[,] MMMM Do[,] YYYY'));
	});
      </script>
