<script>

		function getInfo( type ) {
            var elements = [ 'dopesheet', 'shotlist', 'restrictions' ];

            for ( var i=0; i < elements.length; i++ ) {
                if ($("#" + elements[i]).length > 0) {
                    var visible = type == elements[i] && !$('#' + type).is(':visible');
                    $('#' + elements[i]).toggle(visible);
                    $('#' + elements[i] + '_button').html( visible ? "Close" : "Open" );
                }
			}
		}

        $(document).ready( function() {

            $('[name="downloadable_videos"]').change( function() {
                if ($(this).attr('checked')) {
                    var link = "/download/index/entryId/1_r72n38iw/id/1_r72n38iw/format/" + $(this).attr('value');
					$('#download-link').attr('href', link );
                }
            } );

            var chk = document.getElementsByName('downloadable_videos');
	        var len = chk.length;
	        
	        if ( len > 0 ) {
	        	chk[0].checked = 'checked';
	        	var link = "/download/index/entryId/1_r72n38iw/id/1_r72n38iw/format/" + chk[0].value;
	        	$('#download-link').attr('href', link );
	        }
	        
        } );
        
</script>

