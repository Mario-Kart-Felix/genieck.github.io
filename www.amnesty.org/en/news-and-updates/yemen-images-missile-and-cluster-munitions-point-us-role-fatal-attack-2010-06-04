                        </section>

                        <section class="col input-block">
                            <label class="input-block__label" for="search-advanced--ai-index">Amnesty Index Number</label>
                            <input class="input-block__field" pattern="^[a-zA-Z]{3}\s[0-9]{2,4}\/[0-9]{2,4}\/[0-9]{4}$" placeholder="e.g. &quot;AMR 27/2755/2015&quot;" id="search-advanced--ai-index" type="text" name="ref" title="e.g. &quot;AMR 27/2755/2015&quot;" data-search-input-index-number />
                        </section>

