		function getCookieValue(a) {
			var b = document.cookie.match('(^|[^;]+)\\s*' + a + '\\s*=\\s*([^;]+)');
			var r = b ? b.pop() : '';
			var d = decodeURIComponent(r);
			return d;
		}

		if (navigator.doNotTrack && navigator.doNotTrack === 1) {
			window['ga-disable-UA-1665647-2'] = true;
		}

		if (document.cookie.split(';').filter(function (item) {
			return item.trim().indexOf('CookieControl=') == 0
		}).length) {
			var f = getCookieValue('CookieControl');
			var v = JSON.parse(getCookieValue('CookieControl'));

			if (v.optionalCookies.analytics === "revoked") {
				console.log('Analytics Revoked');
				// Disable Google Analytics
				window['ga-disable-UA-1665647-2'] = true;
			}
			if (v.optionalCookies.marketing === "revoked") {
				console.log('Markerting Revoked');
				// Disable Google Analytics
				if (typeof fbq !== 'undefined') {
					fbq('consent', 'revoke');
				}
			}
		}
