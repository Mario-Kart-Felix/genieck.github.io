<div class="fl-post-carousel-post" itemscope itemtype="https://schema.org/CreativeWork">

	<meta itemscope itemprop="mainEntityOfPage" itemtype="https://schema.org/WebPage" itemid="https://therealnews.com/stories/chelsea-manning-released-but-still-faces-massive-fine" content="Chelsea Manning Released But Still Faces Massive Fine" /><meta itemprop="datePublished" content="2020-03-16" /><meta itemprop="dateModified" content="2020-03-20" /><div itemprop="publisher" itemscope itemtype="https://schema.org/Organization"><meta itemprop="name" content="The Real News Network"><div itemprop="logo" itemscope itemtype="https://schema.org/ImageObject"><meta itemprop="url" content="https://therealnews.com/wp-content/uploads/2017/10/theRealNewsBanner.jpg"></div></div>    <div itemscope itemprop='author' itemtype='https://schema.org/Person'>
      <meta itemprop='url' content='https://therealnews.com/bios/gregory-wilpert'>
      <meta itemprop='name' content='Gregory Wilpert'>
    </div>
    <div itemscope itemprop="image" itemtype="https://schema.org/ImageObject"><meta itemprop="url" content="https://therealnews.com/wp-content/uploads/2020/03/DLY031320_gosztola_manning.jpg" /><meta itemprop="width" content="1280" /><meta itemprop="height" content="720" /></div><div itemprop="interactionStatistic" itemscope itemtype="https://schema.org/InteractionCounter"><meta itemprop="interactionType" content="https://schema.org/CommentAction" /><meta itemprop="userInteractionCount" content="0" /></div>
		<div class="fl-post-carousel-image">
