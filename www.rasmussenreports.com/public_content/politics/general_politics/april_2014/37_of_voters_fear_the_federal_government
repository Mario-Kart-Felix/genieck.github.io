        {
            var fname = $( "#reader_first_name" ).val();
            var lname = $( "#reader_last_name" ).val();
            var email = $( "#reader_email" ).val().replace( "+", "%2B" );
            var signup = $( "#signup" ).attr( "checked" );
            var password = $( "#password" ).val();
            var cpassword = $( "#confirm_password" ).val();

            if( !( fname && lname && email && password && cpassword ) )
            {
                alert( "Please enter your first name, last name, passwords and e-mail address." );
            }
            else if( password.length < 6 || password.length > 20 )
            {
                alert( "The password must be between 6 and 20 characters." );
            }
            else if( password != cpassword )
            {
                alert( "The two given passwords are different." );
            }
            else
            {
                $( "#subscription-result" ).html("");
                $( "#subscribe-loading" ).show();
                $.ajax({
                    type: "POST",
                    url: "/ajax/register-reader",
                    data: "fname=" + fname + "&lname=" + lname + "&email=" + email + "&signup=" + signup + "&password=" + password,
                    success: function(msg){
                        if( msg == "ok" )
                        {
                            $( "#form-container" ).hide();
                            $( "#subscription-result" ).html( "<span class='blue-text'>Congratulations, you have successfully created a trial account. You can now log in with the e-mail address and password specified.</span><br /><br />" );
                        }
                        else if( msg == "exists" )
                        {
                            $( "#subscription-result" ).html( "<span class='blue-text'>You already have an account with that e-mail address.</span><br /><br />" );
                        }
                        else
                        {
                            $( "#subscription-result" ).html( "An error occured subscribing your email. Please try again later<br />" );
                        }
                        $( "#subscribe-loading" ).hide();
                    },
                    error: function(){
                        $( "#subscription-result" ).html( "An error occured subscribing your email. Please try again later<br />" );
                        $( "#subscribe-loading" ).hide();
                    }
                });
            }
        }
    </script>


        </div>
    </div>

