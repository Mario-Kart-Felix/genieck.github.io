  <script>

    var win_w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;

    var loadScript = function(location, callback){
      var fileRef = document.createElement('script');
      fileRef.setAttribute('type','text/javascript');

      if (callback) {
        if (fileRef.readyState) {  // IE
          fileRef.onreadystatechange = function() {
            if (fileRef.readyState == 'loaded' || fileRef.readyState == 'complete') {
              fileRef.onreadystatechange = null;
              callback();
            }
          };
        } else {  // Non-IE
          fileRef.onload = function(){
            callback();
          };
        }
      }

      fileRef.setAttribute('src', location);
      document.head.appendChild(fileRef);
    };


jQuery(document).ready(function ($) {
setTimeout(func, 5000);
function func() {
    loadScript('/circulation/cosmopolitan/scripts/jquery-countdown_v2.min.js', function() {});
}
});

    $(function() {
      var popEnabled,
          popupShown = false;

      
        function getURLParam(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        var referral_source = getURLParam('source'),
            segment_name = getURLParam('segment_name');

        if ( (referral_source != '') && (referral_source != null) ) {
          $("#rescue a").prop("href","https://subscribe.hearstmags.com/subscribe/esquire/259938?source=" + referral_source);
          if ( (segment_name != '') && (segment_name != null) ) {
            $("#rescue a").prop("href","https://subscribe.hearstmags.com/subscribe/esquire/259938?source=" + referral_source + "&segment_name=" + segment_name);
          }
        }
      

      function createCookie(name,value,days) {
        if (days) {
          var date = new Date();
          date.setTime(date.getTime()+(days*24*60*60*1000));
          var expires = "; expires="+date.toGMTString();
        }
        else var expires = "";
        document.cookie = name+"="+value+expires+"; path=/";
      }

      function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1,c.length);
          if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
      }

      var alreadyRescued = readCookie("alreadyRescued_esquire");

      if (alreadyRescued == 'ESQ') {
        popupShown = true;
      }

      function addMinutes(date, minutes) {
        return new Date(date.getTime() + minutes*60000);
      }

      function showPopup() {
        popupShown = true;
        createCookie('alreadyRescued_esquire', 'ESQ', 60);
        // Rescue Impression
        gtag('event', 'view_promotion', {
          'promotions': [
            {
              'id': 'ESQ_Dig_Site_WHLTemplate_Nov19',
              'name': 'Rescue Message',
              'creative_name': 'rescue_message_shown'
            }
          ]
        });

        $("#overlay").css('display','flex').hide().fadeIn(150);

        
          //countdown clock
          var countdownTill = addMinutes(new Date(), 5);
          jQuery('#rescue .countdown').countdown(countdownTill)
          .on('update.countdown', function(event) {
            jQuery(this).html(event.strftime('%M:%S')).fadeIn(200);
          })
          .on('finish.countdown', function(event) {
             gtag('event', 'select_content', {
              'promotions': [
                {
                  'id': 'ESQ_Dig_Site_WHLTemplate_Nov19',
                  'name': 'Rescue Message',
                  'creative_name': 'rescue_message_expired'
                }
              ]
            });
            closePopup();
          });
        
      }

      function closePopup() {
        clearTimeout(popEnabled);
        $("#overlay").fadeOut(150);
      }

      $("#rescue .cls").click(function(){
        // Rescue Exit
        gtag('event', 'select_content', {
          'promotions': [
            {
              'id': 'ESQ_Dig_Site_WHLTemplate_Nov19',
              'name': 'Rescue Message',
              'creative_name': 'rescue_message_closed'
            }
          ]
        });
        closePopup();
      });

      $('#rescue a').click(function() {
        // Rescue Passthrough
        gtag('event', 'select_content', {
          'promotions': [
            {
              'id': 'ESQ_Dig_Site_WHLTemplate_Nov19',
              'name': 'Rescue Message',
              'creative_name': 'rescue_message_passthrough'
            }
          ]
        });
      });

      //disable popup on form input
      $(':input').click(function(){
        clearTimeout(popEnabled);
      });

      
        //timer for desktop and mobile
        if (popupShown === false) {
          //hearst and everyone else
            popEnabled = setTimeout(function(){showPopup();},1000 * 20);
          
        }
      

    });
  </script>

  
  

<script> 
  function circGetURLParam(name, url) {
     if (!url) url = window.location.href;
     name = name.replace(/[\[\]]/g, "\\$&");
     var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
         results = regex.exec(url);
     if (!results) return null;
     if (!results[2]) return '';
     return decodeURIComponent(results[2].replace(/\+/g, " "));
  }
</script>

<!-- CHEETAHMAIL AUTOFILL -->
<script>
  $(function(){
    var cheetahfillform_uc = circGetURLParam('Fform'),
        cheetahfillform_lc = circGetURLParam('fform'),
        cheetahGID = circGetURLParam('GID');

    if (( (cheetahfillform_uc == 'true') || (cheetahfillform_lc == 'true') ) && (cheetahGID != '')) {
      $.ajax({
        type: "POST",
        url: "/subscribe/cheetahMailApi",
        data: {"param": cheetahGID},
        dataType: "json",
        success: function(response) {
          if (response.cheetahmail_success[0] != "") {
            //var ID_country;
            //if (response.cheetahmail_success[0].ID_country == "USA") {
            //  ID_country = "US";
            //}
            $("input[name=first_name]").val(response.cheetahmail_success[0].first_name).text();
            $("input[name=last_name]").val(response.cheetahmail_success[0].last_name).text();
            $("input[name=address]").val(response.cheetahmail_success[0].street_address_1).text();
            $("input[name=address2]").val(response.cheetahmail_success[0].street_address_2).text();
            $("input[name=city]").val(response.cheetahmail_success[0].city).text();
            $("select[name=state]").val(response.cheetahmail_success[0].state_code);
            $("input[name=postal_code]").val(response.cheetahmail_success[0].zip_code.substring(0,5)).text();
            //$("select[name=country]").val(ID_country);
            $("input[name=email]").val(response.cheetahmail_success[0].email_address);
            
            //rodale products
            $(".form-type-textfield").addClass('ready focus');
          }
        }
      });
    }
  });
</script>



