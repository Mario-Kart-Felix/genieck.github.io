<script>
function gocreditcard() {


   jQuery("input[name=payment_method][value=CC]").prop("checked",true);
   jQuery("#cardoption").addClass('cardon');
   jQuery("#paypal-payment").addClass('payment-off');
   jQuery("#amazon-payment").addClass('payment-off');
   jQuery(".payment-change").show();
   jQuery(".payment-arrows").hide();

        if(jQuery("#cardoption").hasClass("cardon")) {
           jQuery("#card-pay-type").show();
           jQuery(".pane-billing-address-form").show();
           jQuery(".panel-col-bottom").show();
           jQuery("input[name=credit_card_number]").prop("required",true);
        }else{
           jQuery("#card-pay-type").hide();
           jQuery(".pane-billing-address-form").hide();
           jQuery(".panel-col-bottom").hide();
           jQuery(".payment-arrows").show();
           jQuery("input[name=credit_card_number]").prop("required",false);
        }

   jQuery(".payment-change").on("click.paychange", function() {
           document.getElementById('cardoption').checked = false;
           jQuery("input[name=credit_card_number]").prop("required",false);
           jQuery(".payment-change").hide();
           jQuery("#cardoption").removeClass('cardon');
           jQuery("#paypal-payment").removeClass('payment-off');
           jQuery("#amazon-payment").removeClass('payment-off');
           jQuery("#card-pay-type").hide();
           jQuery(".pane-billing-address-form").hide();
           jQuery(".panel-col-bottom").hide();
           jQuery(".payment-arrows").show();
       
   });

}

function goamazon() {
jQuery("#amzradio").prop("checked", "true");
jQuery("#OffAmazonPaymentsWidgets0").click();
}

function gopaypal() {
document.cookie= "HEARSTpaytype=a";
jQuery("#paypalradio").prop("checked", "true");

                $('select[name="pt_id"]').val("9");
                $('input[name="pt_id"]').val("9");
                $('[required]').each(function () {
                        $(this).removeAttr('required');
                        console.log(this);
                    }
                );
                $('<input>').attr({
        type: 'hidden',
        name: 'ignore_dav',
        value: "1"
    }).appendTo('form');

jQuery("#frm_control").submit();

}

</script>

 <script>// CC+BML SHOW/HIDE RULES
    $(function() {
      var isInternational = false;

      $("input[name=payment_method]").change(function() {
        var $t = $(this).val();
        if ($t == "BM") {
          $("#edit-payment-details").hide();
          $("#creditcard-payment").hide();
          $(".payment-change-bml").show();

          $("input[name=pt_id]").val(6);
          $("input[name=credit_card_number]").prop("required",false);
        } else {
          $("#edit-payment-details").show();
          $("#creditcard-payment").show();
          $(".payment-change-bml").hide();
          $("input[name=pt_id]").val("");
        }
      });

   jQuery(".payment-change-bml").on("click.paychangebml", function() {
           document.getElementById('payment_method_bm').checked = false;
           document.getElementById('cardoption').checked = false;
           jQuery("#edit-payment-details").show();
           jQuery(".payment-change-bml").hide();
           jQuery(".payment-change").hide();
           jQuery("#cardoption").removeClass('cardon');
           jQuery("#creditcard-payment").show();
           jQuery("#paypal-payment").removeClass('payment-off');
           jQuery("#amazon-payment").removeClass('payment-off');
           jQuery("#card-pay-type").hide();
           jQuery(".pane-billing-address-form").hide();
           jQuery(".panel-col-bottom").hide();
           jQuery(".payment-arrows").show();
       
   });

      $("select[name=country_code]").change(function() {
        var $t = $(this).val();
        if (($t == "US")||($t == "CA")||($t == "")) {
          $("input[name=payment_method][value=BM]").prop("disabled",false);
          $(".xsell_product").show();
          $(".xsell_product input").removeAttr("disabled");
        } else {
          $("#edit-payment-details").show();
          $("input[name=payment_method][value=CC]").prop("checked",true);
          $("input[name=payment_method][value=BM]").prop("disabled",true);
          $("input[name=pt_id]").val("");
          $(".xsell_product").hide();
          $(".xsell_product input").prop("disabled",true);  
        }
      });

   });
  </script>



