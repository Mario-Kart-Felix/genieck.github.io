<script>
    function getConsent() {
        let consent = localStorage.getItem('consentData');
        let isGdpr = localStorage.getItem('gdprApplies');
        return { 'gdpr': isGdpr, 'gdpr_consent': consent };
    }
    function getConsentEncoded() {
        let consent = getConsent();
        let str = '';
        for(name in consent) { str += `&${name}=${consent[name]}` }
        return encodeURIComponent(str);
    }

    jQuery(function() {
        window.__cmp('getConsentData', null, function(result) {
            localStorage.setItem('gdprApplies', result.gdprApplies ? '1' : '0')
            localStorage.setItem('consentData', result.consentData)
        });
    });
</script>


<script>
	var playerAdSize;
	var onlyDesktopDevice = true; //initiate as false
	var isDesktop = MentalFloss.getCurrentDevice() == "desktop";
	var isTablet = MentalFloss.getCurrentDevice() == "tablet";
	var isMobile = MentalFloss.getCurrentDevice() == "mobile";
	if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
		onlyDesktopDevice = false;
	}
	if(isDesktop && onlyDesktopDevice){
		playerAdSize = "640x360";
	}else if(isTablet){
		playerAdSize = "400x225";
	}else if(isMobile){
		playerAdSize = "300x150";
	}
</script>

<script>
    window.showMainNavbar = function() {
		let leaderboardHeight = jQuery('#homepage-top-leaderboard').outerHeight();
		let topAdPositionBottom = jQuery('#homepage-top-leaderboard').offset().top + leaderboardHeight,
				homepageNavPositionBottom = jQuery('#homepage-navbar').offset().top + jQuery('#homepage-navbar').outerHeight(),
				docViewTop = jQuery(window).scrollTop(), navbarClass;
		if(jQuery('#homepage-top-leaderboard').height() > 90) {
			navbarClass = 'show-main-navbar-tall';
		}
		else {
			navbarClass = 'show-main-navbar';
		}
		jQuery('#page').toggleClass(navbarClass, docViewTop > homepageNavPositionBottom);
	}
</script>

