        if (categories.children().length === 1) {
            return;
        }
        
        csl.updateRid(window.ue.rid);
        csl.addWlt('56991');


    }
    spUtils.afterEvent('spATFEvent', executeApsDeepPaginationPleaseRefinePrompt);
});
</script>
