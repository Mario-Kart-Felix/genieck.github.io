				<td width="50" height="27" align="center" bgcolor="#FBF314" id="hour" style="color:#666666; font-weight:normal">        </td>
        <script type="text/javascript">
					var now;
					var timerOut;
					var timerClearOut;
					timerChange();

					function timerChange()
					{
						now=new Date();
						hours=now.getHours();
						minutes=now.getMinutes();
						if(hours<10) hours="0" + hours;
						if(minutes<10) minutes="0" + minutes;
						document.getElementById('hour').innerHTML=hours + ":" + minutes;
						timerOut=setTimeout("timerChange()",1000);
						timerClearOut=setTimeout("timerClear()",500);
					}
					
					function timerClear()
					{
						document.getElementById('hour').innerHTML="";
					}
				</script>
        <td height="25" align="left" id="lastNews" style="padding-left:10px; overflow:hidden"></td>
        <script type="text/javascript">
	var lastNews=new Array();
	var lastNewsNo=0;
	var currentNews=0;
	window.onLoad=getLastNews();
	
	function getLastNews()
	{
		
		lastNews=[
			['Egypt\'s Muslim Brotherhood Students Hold First Conference, Propose Development Initiatives'],['29063'],['Muslim Brotherhood Students Acknowledge Roles of Former Group Members'],['29062'],['Hamas: Abbas UN Bid Forging Fantasies And Illusions'],['29061'],['Syrian MB: Revolution Will Continue Till Oppressive Regime Toppled'],['29060'],['FJP: US Veto of Palestinian State Will Further Harm US-Muslims Relations'],['29059'],['FJP: Representative Parliament Essential to Transition Egypt to Viable Democracy'],['29051'],['FJP Urges Businessmen to Invest in Egypt\'s Promising Tourism Industry'],['29050'],['FJP Leaders Meet Saudi Ambassador in Cairo, Discuss Bilateral Relations'],['29049'],['MB Praises Yemeni Revolution, Calls on Gulf Countries to Intervene'],['29048'],['Demands for Al Azhar to Conduct Elections for All Posts'],['29047'],]
		
		lastNewsNo=20;
		roateLastNews();
	}
	
	var lastNewsTimer;
	
	function roateLastNews()
	{
		document.getElementById('lastNews').innerHTML="<a href='https://web.archive.org/web/20110929043627/http://www.ikhwanweb.com/article.php?id=&quot;&#32;+&#32;lastNews[currentNews+1]&#32;+&#32;&quot;' style='color:#000000'>" + lastNews[currentNews] + "</a>";
		currentNews+=2;
		if(currentNews>=lastNewsNo) currentNews=0;
		lastNewsTimer=setTimeout("roateLastNews()",2200);
	}
</script>
