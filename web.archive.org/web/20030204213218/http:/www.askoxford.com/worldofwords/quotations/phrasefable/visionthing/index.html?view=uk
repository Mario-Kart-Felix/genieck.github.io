<script language="JavaScript" type="text/javascript">
<!-- hide me
function getCookie(label) {
	var labelLen = label.length
	var cLen = document.cookie.length
	var i = 0
	var cEnd
	while (i < cLen) {
		var j = i + labelLen
		if(document.cookie.substring(i,j) == label) {
			cEnd = document.cookie.indexOf(";",j)
			if (cEnd == -1) {
				cEnd = document.cookie.length
			}
			return unescape(document.cookie.substring(j+1,cEnd))
		}
		i++
	}
	return " "
}
// show me-->
</script><!-- SET COOKIE -->
<script language="JavaScript" type="text/javascript">
<!-- hide me
function setCookie(name, value, expires, path, domain, secure) {
	var path = "/"
	var expires = new Date()
	var oneYearFromNow =  expires.getTime() + (365 * 24 * 60 * 60 * 1000)
	expires.setTime(oneYearFromNow)
	document.cookie = name + "=" +escape (value) + 
	((expires) ? "; expires=" + expires.toGMTString() : "") +
	((path) ? "; path=" + path : "") +
	((domain) ? "; domain=" + domain : "") +
	((secure) ? "; secure" : "");
}
// show me-->
</script><script language="Javascript" type="text/javascript">
function killCookie(name, path, domain) {
  var theValue = getCookie(name); // We need the value to kill the cookie
  if(theValue) {
      document.cookie = name + '=' + theValue + '; expires=Fri, 13-Apr-1970 00:00:00 GMT' + ((path)?';path=' + path:'') + ((domain)?';domain=' + domain:''); // set an already-expired cookie
  }
} // killCookie
</script>
<script language="JavaScript" type="text/javascript">
function switchLocation(name,value,expires,path,domain,secure) {
killCookie("Country") // get rid of the old version of this cookie to avoid duplication
setCookie(name, value, expires, path, domain, secure);

	var re = /\?.*/
	var url = document.URL.replace(re, "")                          // remove query string
	var paths = url.split("/")
	var region = getCookie("Country")
	var query = location.search;

// ### Special Cases ###
// these are just some special cases to restrict navigation
// they only apply when somebody switches to an alternate view from a restricted page


if ( (paths[4] == "countdown")  && (region == "get") ) {
	location = "/wordgames/" + "?view=" + getCookie(name) // redirect to wordgames menu
	return true
}

if ( (paths[4] == "name") && (region == "get") ) {
	location = "/worldofwords/" + "?view=" + getCookie(name) // redirect to worldofwords menu
	return true
}

if ( (paths[3] == "shoponline") && (region == "get") ) {
	location = "/shoponline-us/" + "?view=" + getCookie(name) // redirect us users to us catalogue
	return true
}

if ( (paths[3] == "shoponline-us") && (region == "uk") ) {
	location = "/shoponline/" + "?view=" + getCookie(name) // redirect us users to us catalogue
	return true
}

if (paths[3] == "results") {
	location = document.URL
	return true
}


// ### end of Special Cases ###

location = url + "?view=" + getCookie(name)

}
