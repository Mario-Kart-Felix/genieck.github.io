  google_ad_request_done_internal(ads, "", "", "true", "medium_rectangle", "", "", "pgtp=article&kwInURL=true&userAgent=&_s=PM%3ACRIME&pi=53&ci=28642510&cat=justice&pe_id=28642510&tv=ap-CN3-G02&cnn_nav_type=full&rna=pub-2669245315539485&rn=g&rnrci=1702968567&rnrcn=g4-001_ctxt&rncci=6833364212&rnccn=Justice&rnpci=&rnpcn=&rntci=&rntcn=&rnlci=1141114129&rnlcn=1141114129&rnp=c&rt=cpc&paidClick=1&placement_id=ctr-lt-in-top", "", "", "", "", "", "ap-CN3-G02", "CNN");
