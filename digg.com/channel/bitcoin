      const joinNamespace = (e, label) => {
        try {
          ga('send', {
            hitType: 'event',
            eventCategory: 'namespace',
            eventAction: 'join',
            eventLabel: label,
            eventValue: 1,
          });
        } catch (err) {
          console.log(err);
        }

        window.location.href = e.dataset.href;
      }
    </script>
