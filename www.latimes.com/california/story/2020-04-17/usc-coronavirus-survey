    <script id="head-dl">var dataLayer = {"rootId":"","pageTitle":"Less than half L.A. County still has jobs due to coronavirus  - Los Angeles Times","publishDate":"04-17-2020 07:00","source":"latimes","pageName":"lat:california:usc-coronavirus-survey:story.","platform":"22","sectionName":"california","cmsSlug":"usc-coronavirus-survey","leadImage":{"bspContentUuid":"00000171-81d0-d08d-a9f3-dbd702030000","attribution":"(Luis Sinco/Los Angeles Times)"},"ownerSite":"latimes","pageTypeCode":"story","headline":"Less than half of L.A. County residents still have jobs amid coronavirus crisis","urlSlug":"usc-coronavirus-survey","utmSource":"digg","wordCount":892,"bspContentUuid":"00000171-85ce-d269-ab75-c7cea20e0003","bspContentType":"story","adUnit":"GoogleDfp:/21787098806/web.latimes/california","isAccessibleForFree":false,"url":"https://www.latimes.com/california/story/2020-04-17/usc-coronavirus-survey?utm_source=digg","sectionFullPath":"california","tags":"@latimes on Instagram, Coronavirus Pandemic","sitePath":"/","site":"latimes","domain":"www.latimes.com","campaignCodes":"utm_source=digg","permalink":"https://www.latimes.com/california/story/2020-04-17/usc-coronavirus-survey","authors":"Jaclyn Cosgrove"};</script>
