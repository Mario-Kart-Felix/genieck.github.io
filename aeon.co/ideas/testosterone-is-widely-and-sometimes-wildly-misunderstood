<article class='article-card article-card--col-3 article-card--section-philosophy audio' data-align-x='center' data-align-y='center' data-backdrop-strength='3' data-badge-color='white' data-card-play-icon-color='white' data-feature-img-caption-align='right' data-id='3668' data-playing='false'>
<div class='gutter gutter--outer'>
<div class='article-card__inner'>
<div class='article-card__image-wrap' data-image-loaded='false'>
<div data-react-class="Aeon.AnnotatedCorner" data-react-props="{&quot;id&quot;:3668}"></div>
