                                  data-slide=""
                                  data-e2e-name="slide-50"
                                >
                                
                                  <div class="slide-layout clearfix">
                                
                                    <div class="slide-title clearfix" data-e2e-name="slide-title">
                                      <h2 class="slide-title-text">Launched in 2003 by Tiger Electronics, VideoNow was a portable video player targeted at children and teens.</h2>
                                    </div>
                                
                                          
                                            <figure
                                              class="figure image-figure-image "
                                              data-type="img"
                                              data-e2e-name="image-figure-image"
                                              data-media-container="image"
                                            >
                                                <div
                                                  class="lazy-holder lazy-holder "
                                                  style="padding-top: calc(100% * 1465 / 1953)"
                                                >
                                              
                                              
                                              <img
                                                class="lazy-image "
                                                src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
                                                data-content-type="image/png"
                                                data-srcs="{&quot;https://i.insider.com/5e8e2d0b15ea4b592937d327&quot;:{&quot;contentType&quot;:&quot;image/png&quot;,&quot;aspectRatioW&quot;:1953,&quot;aspectRatioH&quot;:1465}}"
                                                alt="VideoNow"
                                              />
                                              
