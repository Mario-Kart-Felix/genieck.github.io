                                  <div
                                    data-bi-ad
                                    id="gpt-slideshow-lifestyle-slideshow_one_page_ad_mobile-fluid-40"
                                    class="ad ad-wrapper fluid postload  only-mobile"
                                    data-force=""
                                    data-type="ad"
                                    data-adunit="insider/lifestyle/slideshow"
