     var self = this;
     whenAvailable("videoLoaded", function() {
      var script = document.createElement("script");
      script.src = self.href;
      document.head.appendChild(script);
     });
    } else {
      var script = document.createElement("script");
      script.src = this.href;
      document.head.appendChild(script);
    }'>
