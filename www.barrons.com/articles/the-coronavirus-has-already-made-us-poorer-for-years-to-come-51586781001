<script>
    var supportsPreload = window.supportsPreload;
    if (!supportsPreload) {
      var optimizelyScript = window.document.createElement('script');
      optimizelyScript.src = 'https://cdn.optimizely.com/public/5090114122/s/rHtkz3QCTR.js';
      optimizelyScript.async = true;
      document.head.appendChild(optimizelyScript);
    }
    setTimeout(optimizelyTimeout, 1000);
    function optimizelyTimeout() {
      window.optimizely = window.optimizely || [];
      if (!window.optimizely.data) {
        window.optimizely.push('timeout');
      }
    }
    if (typeof newrelic !== 'undefined') {
      newrelic.setCustomAttribute('optimizelyClientSide', 1);
    }
</script>

