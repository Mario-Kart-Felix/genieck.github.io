<source srcset="https://static.politico.com/dims4/default/728e301/2147483647/resize/1920x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Ff3%2F30%2F9453ecd740bb843fa9f3cda561b7%2Fwhitmertfc-image8.jpg" media="(min-width: 1681px)">
<source srcset="https://static.politico.com/dims4/default/ef82226/2147483647/resize/1160x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Ff3%2F30%2F9453ecd740bb843fa9f3cda561b7%2Fwhitmertfc-image8.jpg" media="(min-width: 1012px)">
<source srcset="https://static.politico.com/dims4/default/6d19346/2147483647/resize/971x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Ff3%2F30%2F9453ecd740bb843fa9f3cda561b7%2Fwhitmertfc-image8.jpg" media="(min-width: 667px)">
<source srcset="https://static.politico.com/dims4/default/2f7c761/2147483647/resize/646x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Ff3%2F30%2F9453ecd740bb843fa9f3cda561b7%2Fwhitmertfc-image8.jpg" media="(min-width: 485px)">
<source srcset="https://static.politico.com/dims4/default/ba486c5/2147483647/resize/463x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Ff3%2F30%2F9453ecd740bb843fa9f3cda561b7%2Fwhitmertfc-image8.jpg" media="(max-width: 484px)">
