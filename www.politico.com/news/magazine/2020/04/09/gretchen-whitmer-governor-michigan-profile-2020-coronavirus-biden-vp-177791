<source srcset="https://static.politico.com/dims4/default/701b796/2147483647/resize/1920x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F3a%2Fef%2F7ce30e75487084a2f57440bf8636%2Fwhitmertfc-image10.jpg" media="(min-width: 1681px)">
<source srcset="https://static.politico.com/dims4/default/ac1be40/2147483647/resize/1160x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F3a%2Fef%2F7ce30e75487084a2f57440bf8636%2Fwhitmertfc-image10.jpg" media="(min-width: 1012px)">
<source srcset="https://static.politico.com/dims4/default/e162a16/2147483647/resize/971x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F3a%2Fef%2F7ce30e75487084a2f57440bf8636%2Fwhitmertfc-image10.jpg" media="(min-width: 667px)">
<source srcset="https://static.politico.com/dims4/default/656fb7e/2147483647/resize/646x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F3a%2Fef%2F7ce30e75487084a2f57440bf8636%2Fwhitmertfc-image10.jpg" media="(min-width: 485px)">
<source srcset="https://static.politico.com/dims4/default/a1eda20/2147483647/resize/463x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F3a%2Fef%2F7ce30e75487084a2f57440bf8636%2Fwhitmertfc-image10.jpg" media="(max-width: 484px)">
