<source srcset="https://static.politico.com/dims4/default/f581817/2147483647/resize/1920x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fcc%2Fea%2F2429801245d4ad97f00dfefcf788%2Fwhitmertfc-image6.jpg" media="(min-width: 1681px)">
<source srcset="https://static.politico.com/dims4/default/3a602b4/2147483647/resize/1160x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fcc%2Fea%2F2429801245d4ad97f00dfefcf788%2Fwhitmertfc-image6.jpg" media="(min-width: 1012px)">
<source srcset="https://static.politico.com/dims4/default/09187d3/2147483647/resize/971x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fcc%2Fea%2F2429801245d4ad97f00dfefcf788%2Fwhitmertfc-image6.jpg" media="(min-width: 667px)">
<source srcset="https://static.politico.com/dims4/default/723d0ed/2147483647/resize/646x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fcc%2Fea%2F2429801245d4ad97f00dfefcf788%2Fwhitmertfc-image6.jpg" media="(min-width: 485px)">
<source srcset="https://static.politico.com/dims4/default/406ed3f/2147483647/resize/463x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fcc%2Fea%2F2429801245d4ad97f00dfefcf788%2Fwhitmertfc-image6.jpg" media="(max-width: 484px)">
<img src="https://static.politico.com/cc/ea/2429801245d4ad97f00dfefcf788/whitmertfc-image6.jpg" alt="Five-year-old Gretchen Whitmer (second from left) with her mother, grandmother and sister." title="Five-year-old Gretchen Whitmer (second from left) with her mother, grandmother and sister."></picture>
</div>
<figcaption class="story-photo__caption">
<p>
Five-year-old Gretchen Whitmer (second from left) with her mother, grandmother and sister. | Provided </p>
