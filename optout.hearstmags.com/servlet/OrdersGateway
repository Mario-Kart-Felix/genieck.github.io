	<script type="text/javascript">
		$(document).ready(function(){
			
			/* Client ID Start */
			function client_id () {
				function getUrlVars() {
					var vars = {};
					var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
					vars[key] = value;
					});
					return vars;
				}
				var param_client_id = getUrlVars()["client_id"];
				if (typeof param_client_id != 'undefined')	{
					document.location='/servlet/OrdersGateway?cds_mag_code=A99&cds_page_id=246801&cds_misc_1=' + param_client_id;
				}
				else {
					$('#clientID').show();
				}
			} client_id();
			/* Client ID End */
			
			/* Multi-Select Start */			
			hearstProducts = new Array();
			hearstProducts2 = new Array();
			
			$('input[name="prodques1"]').on('change', function(){  
				if($(this).val()  === "Y") {
					$('#prodshow1').collapse('show');
					$(".multi").select2({
					closeOnSelect: false,
					templateResult: formatState,
					placeholder: 'Product Titles *'
				});
				}
				else {
				 	$('#prodshow1').collapse('hide');
					$('#prodsel1 select').val(null).trigger('change');
					$('#prodsel1 .counter').hide();
					$('#prodsel1 .select2-results__option').attr('aria-selected', 'false');
					$('input[name=cds_company]').val("");
				}
			});	
			
			$('input[name="prodques2"]').on('change', function(){  
				if($(this).val()  === "Y") {
					$('#prodshow2').collapse('show');
					$(".multi").select2({
					closeOnSelect: false,
					templateResult: formatState,
					placeholder: 'Product Titles *'
				});
				}
				else {
				 	$('#prodshow2').collapse('hide');
					$('#prodsel2 select').val(null).trigger('change');
					$('#prodsel2 .counter').hide();
					$('#prodsel2 .select2-results__option').attr('aria-selected', 'false');
					$('input[name=cds_company]').val("");
				}
			});			
			
			$("#prodsel1 select").on('select2:open', function (e) {
				if ($('.clearAll').length === 0) {
					$(".select2-results").append("<a href='#' class='clearAll'>Clear All</a>");
				}
				parentThis = this;
				$(".clearAll").on('click', function (e) {
					e.preventDefault();
					$('#prodsel1 select').val(null).trigger('change');
					$(parentThis).parent().find('.counter').hide();
					$(this).parent().find('.select2-results__option').attr('aria-selected', 'false');
					$('#cds_onetrust_mags1').val('');
				});
			});
			
			$("#prodsel1 select").on('select2:select select2:unselect', function (e) {
				//var id = e.params.data.id;
			    //var option = $(e.target).children('[value='+id+']');
			    //option.detach();
			    //$(e.target).append(option).change();
				var selections = $('#prodsel1 .select2-selection__choice').length;
				if (selections >= 1) {
			  		$(this).parent().find('.counter').text(selections);
					$(this).parent().find('.counter').show();
				}
				else {
					$(this).parent().find('.counter').hide();
				}
				
				$('.select2-search__field').val("");
				
				hearstProducts = [];
      			var prodData = JSON.stringify($('#prodsel1 select').select2('data'));
				data = $.parseJSON(prodData);
				$(data).each( function () {
					hearstProducts.push(this.id);
				});
				
				$('#cds_onetrust_mags1').val(hearstProducts);
				
			});
			
			$("#prodsel2 select").on('select2:open', function (e) {
				if ($('.clearAll2').length === 0) {
					$(".select2-results").append("<a href='#' class='clearAll2'>Clear All</a>");
				}
				parentThis = this;
				$(".clearAll2").on('click', function (e) {
					e.preventDefault();
					$('#prodsel2 select').val(null).trigger('change');
					$(parentThis).parent().find('.counter').hide();
					$(this).parent().find('.select2-results__option').attr('aria-selected', 'false');
					$('#cds_onetrust_mags2').val('');
				});
			});
			
			$("#prodsel2 select").on('select2:select select2:unselect', function (e) {
				//var id = e.params.data.id;
			    //var option = $(e.target).children('[value='+id+']');
			    //option.detach();
			    //$(e.target).append(option).change();
				var selections = $('#prodsel2 .select2-selection__choice').length;
				if (selections >= 1) {
			  		$(this).parent().find('.counter').text(selections);
					$(this).parent().find('.counter').show();
				}
				else {
					$(this).parent().find('.counter').hide();
				}
				
				$('.select2-search__field').val("");
				
				hearstProducts2 = [];
      			var prodData = JSON.stringify($('#prodsel2 select').select2('data'));
				data = $.parseJSON(prodData);
				$(data).each( function () {
					hearstProducts2.push(this.id);
				});
				
				$('#cds_onetrust_mags2').val(hearstProducts2);
			});	

			function formatState (state) {
			if (!state.id) { return state.text; }
				var $state = $(
					'<span><div></div>' + state.text + '</span>'
				);
				return $state;
			}
			
			$.fn.select2.amd.require(['select2/selection/search'], function (Search) {
				var oldRemoveChoice = Search.prototype.searchRemoveChoice;

				Search.prototype.searchRemoveChoice = function () {
					oldRemoveChoice.apply(this, arguments);
					this.$search.val('');
				};
			});
			
			$(".multi").select2().change(function() {
				$(this).valid();
			});
			
			$(".multi").on('select2:open', function () {
				$(this).parent().find('div.error').hide();
			});
			
			$(".multi").on('select2:close', function () {
				if ($(this).parent().find('.counter').is(':visible')) {
					// Do nothing
				}
				else {
					$(this).parent().find('div.error').show();
				}
			});
			
			var retrProds1 = $('#cds_onetrust_mags1').val();
			var retrProds2 = $('#cds_onetrust_mags2').val();
			
			if (retrProds1 != "") {
				var result1 = retrProds1.split(',');
				$('#prodques1a').click();
				$('#prodsel1 select').val(result1).trigger('change');
			}
			else if ($('.server_side').length) {
				$('#prodques1b').click();
			}
			
			if (retrProds2 != "") {
				var result2 = retrProds2.split(',');
				$('#prodques2a').click();
				$('#prodsel2 select').val(result2).trigger('change');
			}
			else if ($('.server_side').length) {
				$('#prodques2b').click();
			}
			
			$(".multi").on("select2:unselect", function (evt) {
  				if (!evt.params.originalEvent) {
    				return;
  				}
				// Stop dropdown from closing un unselect
  				evt.params.originalEvent.stopPropagation();
			});
			/* Multi-Select End */
			
			/* Account Number Number Only Start */
			(function($) {
			  $.fn.inputFilter = function(inputFilter) {
				return this.on("input keydown keyup mousedown mouseup select contextmenu drop", function() {
				  if (inputFilter(this.value)) {
					this.oldValue = this.value;
					this.oldSelectionStart = this.selectionStart;
					this.oldSelectionEnd = this.selectionEnd;
				  } 
				else if (this.hasOwnProperty("oldValue")) {
					this.value = this.oldValue;
					this.setSelectionRange(this.oldSelectionStart, this.oldSelectionEnd);
				  } else {
					this.value = "";
				  }
				});
			  };
			}(jQuery));
			
			$(".numberonly").inputFilter(function(value) {
				return /^\d*$/.test(value);    // Allow digits only, using a RegExp
			});
			/* Account Number Number Only End */
			
			/* Credit Card Masking Start */
			$(function () {
				$("#cds_cc_number").inputmask({"mask": "9999 9999 9999 9999", "autoUnmask": true, "removeMaskOnSubmit": true});

				$("#cds_cc_number").on("keyup", function (event) {
					var value = $(this).inputmask("unmaskedvalue");

					if (value.length === 2 && (value === "37" || value === "34")) {
						$("#cds_cc_number").inputmask({"mask": "9999 999999 99999", "autoUnmask": true, "removeMaskOnSubmit": true});
					}
					else {
						$("#cds_cc_number").inputmask({"mask": "9999 9999 9999 9999", "autoUnmask": true, "removeMaskOnSubmit": true});
					}
				});
			});
			/* Credit Card Masking End */
			
			/* Credit Card Detector Start */
			$('#cds_cc_number').creditCardTypeDetector({ 'credit_card_logos' : '.card_logos' });
			/* Credit Card Detector End */
			
			/* Fix - Floating Labels for Selects Start */
			$('select:not(.multi)').attr('value', $('select option:selected').val());
			$('select:not(.multi)').on('keyup change', function() {
				$(this).attr('value', $("option:selected", this).val());
			});
			/* Fix - Floating Labels for Selects End */
			
			/* Update State List for IE/Edge Start */
			var ua = window.navigator.userAgent;
			var isIE = /MSIE|Trident|Edge\//.test(ua);
			if ( isIE ) {
			  $("select[name='cds_state'] option[value='']").text('State');
			}
			/* Update State List for IE/Edge End */
			
			/* Auto Renewal Copy Start */
			function AR() {
				var selTV = $('input[name=cds_term_value]:checked').val();
				if (selTV == "1") {
					$('.ARterm').text("$3.99 each month");
				}
				else {
					$('.ARterm').text("$39.00 each year");
				}
			}
			$("input[name=cds_term_value]").change(function(){
				AR();
			}); AR();
			/* Auto Renewal Copy End */
			
			/* Server Side Error Replacement Start */
			$(".server_side").contents().filter(function() {
      			return this.nodeType === 3;
    		}).wrap("<span></span>").end().filter("br").remove();
			
			$('.server_side span').each(function() {
				if ($(this).text() == "You are already a customer of this product. If you find this to be incorrect, please call customer service.") {
					$(this).html('You have already submitted a request. If you wish to change your request please email <a href="mailto:ccpamagsrequests@hearst.com">ccpamagsrequests@hearst.com</a>.');
				}
			});
			/* Server Side Error Replacement End */
			
			$("form").submit(function(){
				if($(this).valid()) {
					var thisForm = this;
				setTimeout( function(){ 
					$('.btn').prop('disabled', true);
					$('.btn').html('<span class="button-spinner"><img src="images/AdsFree/spinner.svg" /></span> Complete Request');
				}, 0);
				}
			});
			
			/* Validation Start */
			//$(".form-control").click(function() {
			//	$(this).parent().find('div.error').hide();
  			//	$(this).removeClass("error");
			//});
			
			$('form').each(function () {	
				var validator = $(this).validate({
					errorElement: 'div',
					errorPlacement: function(error, element) {
						if(element.parent('.form-label-group-select2').length) {
							error.insertAfter(element.parent().find('span.select2'));
						}
						else if (element.parent('.custom-control').length) {
							error.insertAfter(element.parent().parent());
						}
						else {
							error.insertAfter(element);
						}
					},
					 highlight: function (element, errorClass, validClass) {
						var elem = $(element);
						if (elem.hasClass("multi")) {
							elem.parent().find('.select2-selection--multiple').addClass(errorClass);
						} else {
							elem.addClass(errorClass);
						}
					},
					unhighlight: function (element, errorClass, validClass) {
						var elem = $(element);
						if (elem.hasClass("multi")) {
							elem.parent().find('.select2-selection--multiple').removeClass(errorClass);
						} else {
							elem.removeClass(errorClass);
						}
					},
					rules: {
						cds_first_name: {required: true},
						cds_last_name: {required: true},
						cds_email: {required: true, email: true},
						prodques1: {required: true},
						prodques2: {required: true},
						cds_company: {required: true, minlength: 5},
						prods1: {required: true},
						prods2: {required: true},
						cds_address_1: {required: true},
						cds_city: {required: true},
						cds_state: {required: true},
						cds_zip: {required: true, minlength: 5},
						cds_cc_number: {required: true, creditcard: true},
						cds_cc_exp_month: {required: true, minlength: 2},
						cds_cc_exp_year: {required: true, minlength: 2},
						cds_cc_security_code: {required: true, minlength: 3}
					},
					messages: {
						cds_first_name: {required: "Please enter a first name."},
