                        window.onload = function() {
                            var $targetElement = 'input#edit-search';
                            jQuery($targetElement).attr('autocomplete', 'off');

                            jQuery('input#edit-reset').on('click', function (e) {
                                e.preventDefault();
                                jQuery($targetElement).val('').focus();
                            });
                            jQuery('[data-toggle="collapse"]').on('click', function (e) {
                                e.preventDefault();
                                var $this = jQuery(this);

                                var targetID = $this.attr('aria-controls');
                                var $target = jQuery('#' + targetID);

                                // Toggle the target element
                                $target.toggle(0, function () {
                                    // Update the aria tags
                                    var visible = $target.is(':visible');
                                    $this.attr('aria-expanded', visible);
                                    $target.toggleClass('in', visible);
                                });
