        istats.addLabels({
          'pal_route': 'webservice',
          'ml_name': 'barlesque',
          'app_type': 'responsive',
          'language': 'en-GB',
          'ml_version': '0.28.7',
          'pal_webapp': 'barlesque',
          'prod_name': 'frameworks',
          'blq_s': '4d',
          'blq_r': '2.7',
          'blq_v': 'default',
          'blq_e': 'webservice-HTML',
          'bbc_mc': (c ? 'ad' + c.charAt(0) + 'ps' + c.charAt(1) + 'pf' + c.charAt(2) : 'not_set'),
          'screen_resolution': screenWidthAndHeight,
          'ns_referrer': encodeURI(((window.orb.referrer) ? window.orb.referrer : document.referrer)),
          'app_version': buildNumber,
          'app_name': 'bbc-three',
          'bbc_site': 'bbcthree',
          'page_type': currentType
        });
      });
    }
  }();
</script>

