ecovered.jpeg?crop=1xw:0.9299719887955181xh;center,center","section":null,"original_channel":null,"topics":[{"type":"id","generated":false,"id":"Topic:57a2063bce6c3edef347cfe7","typename":"Topic"},{"type":"id","generated":false,"id":"Topic:57c640db421d6781e9572876","typename":"Topic"},{"type":"id","generated":false,"id":"Topic:58477f301add200286af6d0a","typename":"Topic"},{"type":"id","generated":false,"id":"Topic:5e4538e283dba1009886f1ac","typename":"Topic"}],"topic_callout":null,"primary_topic":{"type":"id","generated":false,"id":"Topic:57a204a98cb727dec7951877","typename":"Topic"},"contributions":[{"type":"id","generated":true,"id":"Article:5e8b5e0eece155009d77db03.contributions.0","typename":"Contribution"},{"type":"id","generated":true,"id":"Article:5e8b5e0eece155009d77db03.contributions.1","typename":"Contribution"}],"__typename":"Article","social_lede":null},"Topic:57a2063bce6c3edef347cfe7":{"name":"Hurricane","id":"57a2063bce6c3edef347cfe7","slug":"hurricane","__typename":"Topic"},"Topic:57c640db421d6781e9572876":{"name":"fema","id":"57c640db421d6781e9572876","slug":"fema","__typename":"Topic"},"Topic:58477f301add200286af6d0a":{"name":"Coronavirus","id":"58477f301add200286af6d0a","slug":"coronavirus","__typename":"Topic"},"Topic:5e4538e283dba1009886f1ac":{"name":"COVID-19","id":"5e4538e283dba1009886f1ac","slug":"covid-19","__typename":"Topic"},"Topic:57a204a98cb727dec7951877":{"name":"Money","id":"57a204a98cb727dec7951877","slug":"money","__typename":"Topic"},"Article:5e8b5e0eece155009d77db03.contributions.0":{"role":"Author","role_id":"589b62267be1070395990030","contributor":{"type":"id","generated":false,"id":"Contributor:5e8b5f6e891208009d197ec3","typename":"Contributor"},"__typename":"Contribution"},"Contributor:5e8b5f6e891208009d197ec3":{"first_name":"Elena","last_name":"Botella","full_name":"Elena Botella","url":"https:\u002F\u002Fwww.vice.com\u002Fen_us\u002Fcontributor\u002Felena-botella","slug":"elena-botella","id":"5e8b5f6e891208009d197ec3","__type